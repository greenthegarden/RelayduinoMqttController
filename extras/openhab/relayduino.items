Group     gRelayduino

Switch    RelayduinoDiscovered           "On Network"                                     (gRelayduino)  {nh="192.168.2.90"} 

// input values from Relayduino
String    RelayduinoRelayState           "[%s]"                                           (gRelayduino)  {mqtt="<[openhab:relayduino/status/relay:state:default]"}
String    RelayduinoAnalogInput          "[%s]"                                           (gRelayduino)  {mqtt="<[openhab:relayduino/input/analog_in:state:default]"}
String    RelayduinoDigitalInput         "[%s]"                                           (gRelayduino)  {mqtt="<[openhab:relayduino/input/opto_input:state:default]"}

// relay control messages to Relayduino
String    RelayduinoRelayControl         "[%s]"                                           (gRelayduino)  {mqtt=">[openhab:relayduino/control/relay:command:*:default]"}

// relay states
Switch    RelayduinoRelay1State          "Relay 1 Running"                                (gRelayduino)
DateTime  RelayduinoRelay1LastOn         "Last on [%1$tT, %1$td.%1$tm.%1$tY]"
DateTime  RelayduinoRelay1LastOff        "Last off [%1$tT, %1$td.%1$tm.%1$tY]"

