// input values
Number    RelayduinoAnalogInput1         "Analog Input 1 [%.1f]"
Number    RelayduinoAnalogInput2         "Analog Input 2 [%.1f]"
Number    RelayduinoAnalogInput3         "Analog Input 3 [%.1f]"
Number    RelayduinoAnalogInput4         "Analog Input 4 [%.1f]"
Switch    RelayduinoDigitalInput1        "Digital Input 1"
Switch    RelayduinoDigitalInput2        "Digital Input 2"
Switch    RelayduinoDigitalInput3        "Digital Input 3"

Group gRelayduinoEvents
String    gRelayduinoCommandEventName1   "Event [%s]"                         <calendar>  (gRelayduinoEvents)  {caldavPersonal="calendar:openhab type:EVENT eventNr:1 value:NAME"}
